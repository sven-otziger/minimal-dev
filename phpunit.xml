<?xml version="1.0"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" bootstrap="bootstrap.php" colors="true"
         convertErrorsToExceptions="true" convertNoticesToExceptions="false" convertWarningsToExceptions="false"
         backupGlobals="false" backupStaticAttributes="false"
         xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/9.3/phpunit.xsd">

    <testsuites>
        <testsuite name="unit">
            <directory suffix="Test.php">test</directory>
        </testsuite>
    </testsuites>
</phpunit>
