{
    "name": "sven/demo",
    "autoload": {
        "psr-4": {
            "Sven\\Demo\\": "src/",
            "App\\": "src/App",
            "Test\\": "src/App/Test",
            "Controller\\": "src/App/Controller",
            "Service\\": "src/App/service",
            "Retro\\Constructor\\": "src/Retro/Constructor",
            "Retro\\Namespaces\\PC\\": "src/Retro/Namespaces/PC",
            "Retro\\Namespaces\\TV\\": "src/Retro/Namespaces/TV",
            "Retro\\Static\\": "src/Retro/Static",
            "Retro\\Inheritance\\": "src/Retro/Inheritance",
            "Retro\\Serialization\\": "src/Retro/Serialization",
            "Entity\\": "src/App/Entity",
            "Repository\\": "src/App/Repository",
            "Handler\\": "src/App/Handler",
            "Enum\\": "src/App/Enum",
            "Exception\\": "src/App/Exception"
        }
    },
    "authors": [
        {
            "name": "Sven Otziger",
            "email": "sven.otziger@payrexx.com"
        }
    ],
    "require": {
        "filp/whoops": "^2.14",
        "symfony/routing": "^6.0",
        "robmorgan/phinx": "^0.12.10",
        "twig/twig": "^3.4",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "doctrine/annotations": "1.13.2",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "5.4.x-dev",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "doctrine": "vendor/bin/doctrine",
        "test": "vendor/bin/phpunit -c phpunit.xml --no-coverage --verbose"
    }
}
